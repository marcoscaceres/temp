{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
    "blockBindings": true,
    "arrowFunctions": true,
    "destructuring": true,
    "generators": true,
    "spread": true,
    "restParams": true,
    "objectLiteralShorthandMethods": true
  },
  "rules": {
    "generator-star-spacing": [2, "after"]
  }
}
